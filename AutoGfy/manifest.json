{
  "manifest_version": 2,

  "name": "AutoGfy",
  "description": "Converts all gif links into gfycat links.",
  "version": "1.2",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "run_at": "document_end",
      "js": ["jquery.js", "js-extend.js", "autogfy.js"]
    }
  ]
}